{
  "manifest_version": 3,
  "name": "ChatGPT Chrome Extension",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["activeTab", "http://localhost:5000/"]
}